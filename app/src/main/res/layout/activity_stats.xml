<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.danbi_000.waitix.StatsActivity">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_menuLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ff393c46"
            android:gravity="left"
            android:orientation="vertical"
            android:textColor="#ff000000">

            <!-- include -->

            <include
                android:id="@+id/ic_leftslidemenu"
                layout="@layout/slidemenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_mainlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:gravity="left"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/ll_fragment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="top"
                    android:background="#ffffffff">

                    <RelativeLayout
                        android:id="@+id/relativeLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_gravity="top"
                        android:background="#373f4a">

                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:layout_marginTop="15dp"
                            android:text="대기 번호 통계"
                            android:textColor="#ffffff"
                            android:textSize="16dp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/btn_menu"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:padding="15dp"
                            android:src="@drawable/store_menu"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/relativeLayout2"
                        android:layout_centerVertical="true"
                        android:background="#f4f2f8">

                        <RelativeLayout
                            android:id="@+id/btnWeek"
                            android:layout_width="180dp"
                            android:layout_height="40dp"
                            android:background="#bbbbbb">

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/btnMonth"
                            android:layout_width="180dp"
                            android:layout_height="40dp"
                            android:background="#cccccc"
                            android:layout_toRightOf="@+id/btnWeek">

                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_below="@+id/btnWeek"
                            android:background="@drawable/store_statics_bg"
                            android:layout_margin="10dp">
                            <RelativeLayout
                                android:id="@+id/rl1"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_marginLeft="30dp">
                                <TextView
                                    android:id="@+id/tv_totalNum"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_totalNum"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="총 발급 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rl2"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="30dp">
                                <TextView
                                    android:id="@+id/tv_totalCancelNum"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView2"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_totalCancelNum"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView2"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="총 발급 취소 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:id="@+id/rl3"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_marginLeft="30dp"
                                android:layout_below="@+id/rl1"
                                android:layout_marginTop="20dp">
                                <TextView
                                    android:id="@+id/tv_mobileNum"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView3"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_mobileNum"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView3"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="앱 발급 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rl4"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="30dp"
                                android:layout_alignTop="@+id/rl3">
                                <TextView
                                    android:id="@+id/tv_offlineNum"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView4"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_offlineNum"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView4"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="오프라인 발급 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:id="@+id/rl5"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_marginLeft="30dp"
                                android:layout_below="@+id/rl3"
                                android:layout_marginTop="20dp">
                                <TextView
                                    android:id="@+id/tv_averageTime"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_averageTime"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView5"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="평균 대기시간(분)"
                                    android:textColor="#888888"/>
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rl6"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="30dp"
                                android:layout_alignTop="@+id/rl5">
                                <TextView
                                    android:id="@+id/tv_averageNum"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView6"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_averageNum"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView6"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="평균 발급 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:id="@+id/rl7"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_marginLeft="30dp"
                                android:layout_below="@+id/rl5"
                                android:layout_marginTop="20dp">
                                <TextView
                                    android:id="@+id/tv_enterTeam"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView7"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_enterTeam"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView7"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="총 입장 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rl8"
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:gravity="center"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="30dp"
                                android:layout_alignTop="@+id/rl7">
                                <TextView
                                    android:id="@+id/tv_cancelTeam"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="000"
                                    android:textSize="40sp"/>
                                <ImageView
                                    android:id="@+id/imageView8"
                                    android:layout_width="30dp"
                                    android:layout_height="0.5dp"
                                    android:layout_below="@+id/tv_cancelTeam"
                                    android:layout_marginTop="10dp"
                                    android:layout_centerHorizontal="true"
                                    android:background="@drawable/store_initial_line_b"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/imageView8"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:text="총 미입장 수"
                                    android:textColor="#888888"/>
                            </RelativeLayout>
                        </RelativeLayout>
                       </RelativeLayout>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/ll_empty"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:orientation="vertical">
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
